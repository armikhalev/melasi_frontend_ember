<div class="latayPage">
    {{#each cards as |card|}}
        <div class="flipContainer {{if card.data.flip "flip"}}" {{action "flipCard" card}}> 
            <div class="flipper">
                <div class="card frontCard">{{card.front}}</div>
                <div class="card backCard">{{card.back}}</div>
            </div>
        </div>
    {{/each}}
</div>
